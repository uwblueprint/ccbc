{
  "development": {
    "use_env_variable": "DATABASE_URL"
  },
  "test": {
    "use_env_variable": "DATABASE_URL"
  },
  "production": {
    "use_env_variable": "DATABASE_URL",
    "dialect": "postgres",
    "protocol": "postgres",
    "ssl": true,
    "dialectOptions": {
      "ssl": {
       "require": true,
       "rejectUnauthorized": false
      }
    }
  }
}
